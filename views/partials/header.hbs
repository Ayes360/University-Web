
<header class="text-gray-600 body-font bg-blue-900">
        <div class="container mx-auto flex flex-wrap p-5 flex-col md:flex-row items-center">
            <a class="flex title-font font-medium items-center text-gray-900 mb-4 md:mb-0">
                <img src="{{data.brandIconUrl}}" alt="Air University Logo" class="w-10 h-10 rounded-full object-cover">
                <span class="ml-3 text-xl text-white">{{data.brandName}}</span>
              </a>
          <nav class="md:ml-auto md:mr-auto flex flex-wrap items-center text-base justify-center">
            <!--This is an hbs loop which will get data from the data base to load here in the navbar-->
            {{#each data.links}}
            <a class="mr-5 hover:bg-blue-500 text-white" href="{{this.url}}">{{this.lable}}</a>
            {{/each}}
          </nav>
          <!--Login-->
          <a href="Login.html" class="inline-flex items-center border-0 py-1 px-3 focus:outline-none hover:bg-blue-500 rounded text-base mt-4 md:mt-0 mr-4 text-white">
            Login
            <svg class="w-4 h-4 ml-2" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round">
                <path d="M5 12h14M12 5l7 7-7 7"></path>
            </svg>
        </a>
          <!--Signup-->
          <!-- <button class="inline-flex items-center bg-gray-100 border-0 py-1 px-3 focus:outline-none hover:bg-gray-200 rounded text-base mt-4 md:mt-0 text-blue-900">Sign Up
              <path d="M5 12h14M12 5l7 7-7 7"></path>
            </svg>
          </button>
        </div> -->
        
      <a href="signup.html" class="inline-flex items-center bg-gray-100 border-0 py-3 px-3 focus:outline-none hover:bg-gray-200 rounded text-base mt-4 md:mt-0 text-blue-900">
        Signup
        <svg class="w-4 h-4 ml-2" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round">
            <path d="M5 12h14M12 5l7 7-7 7"></path>
        </svg>
    </a>
      </header>
